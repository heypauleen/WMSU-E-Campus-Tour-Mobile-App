<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@mipmap/pagefourbg"
    android:orientation="vertical"
    android:paddingTop="100dp"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnboardofregents"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton1"
            android:text="Board of Regents"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
   </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnadmin"
            android:layout_width="270dp"
            android:layout_height="90dp"
            android:background="@mipmap/bgbutton2"
            android:text="Administrative Officials"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btndeans"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton3"
            android:text="Academic Deans"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>





</LinearLayout>