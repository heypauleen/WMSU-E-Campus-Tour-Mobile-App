<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@mipmap/pagethreebg"
    android:orientation="vertical"
    android:paddingTop="100dp"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="13dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnvisionmission"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton1"
            android:text="Vision Mission"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
   </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnhistory"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton2"
            android:text="History"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnstrategicplan"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton3"
            android:text="Strategic Plan"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btntransparencyseal"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton4"
            android:text="Transparency seal"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnnews"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton1"
            android:text="News Archive"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btncalendar"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton2"
            android:text="Academic Calendar"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="1dp"
        android:gravity="center_horizontal">
        <Button
            android:id="@+id/btnqualitypolicy"
            android:layout_width="270dp"
            android:layout_height="70dp"
            android:background="@mipmap/bgbutton3"
            android:text="Quality Policy"
            android:fontFamily="@font/cocogoose"
            android:textColor="#FFFFFF"
            android:textSize="20dp"/>
    </LinearLayout>




</LinearLayout>